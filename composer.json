{
    "name": "liip/open-swiss-building-api",
    "type": "project",
    "license": "MIT",
    "version": "1.0.0",
    "description": "A set of APIs to expose the open.swiss Bulding data",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo_pgsql": "*",
        "ext-pdo_sqlite": "*",
        "ext-zip": "*",
        "brick/geo": "^0.13.0",
        "composer/pcre": "^3.1",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.5",
        "dragonmantank/cron-expression": "^3.4",
        "jsor/doctrine-postgis": "dev-dbal-deprecations#9a88d1f66372e51bcd5b92fdef3a03306096a293 as v2.3",
        "league/csv": "^9.22",
        "meilisearch/meilisearch-php": "^1.13",
        "nelmio/api-doc-bundle": "^5.0",
        "nyholm/dsn": "^2.0",
        "nyholm/psr7": "^1.8",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^2.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/doctrine-messenger": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "*",
        "symfony/messenger": "*",
        "symfony/property-access": "*",
        "symfony/property-info": "*",
        "symfony/runtime": "*",
        "symfony/scheduler": "*",
        "symfony/serializer": "*",
        "symfony/string": "*",
        "symfony/twig-bundle": "*",
        "symfony/uid": "*",
        "symfony/validator": "*",
        "symfony/yaml": "*",
        "twig/extra-bundle": "^3.8",
        "twig/twig": "^3.8"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "rector/rector": "^2.0",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "*",
        "symfony/maker-bundle": "^1.52",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "cs:check": "PHP_CS_FIXER_IGNORE_ENV=1 tools/php-cs-fixer.phar fix --dry-run --diff",
        "cs:fix": "PHP_CS_FIXER_IGNORE_ENV=1 tools/php-cs-fixer.phar fix --verbose",
        "phive:install": "phive --no-progress install --trust-gpg-keys 0x4AA394086372C20A,0xE82B2FB314E9906E",
        "phpstan:check": "vendor/bin/phpstan analyze",
        "rector:check": "vendor/bin/rector --dry-run",
        "rector:fix": "vendor/bin/rector",
        "phpunit:check": "tools/phpunit.phar",
        "phpunit:coverage:html": "XDEBUG_MODE=develop,coverage tools/phpunit.phar --colors=always --coverage-html var/cache/ci/coverage"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        }
    },
    "repositories": [{
        "type": "github",
        "url": "https://github.com/GwendolenLynch/doctrine-postgis"
    }]
}
